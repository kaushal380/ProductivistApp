{
  "expo": {
    "name": "Productivist",
    "slug": "Productivist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/launcherIcon.png",
    "splash": {
      "image": "./assets/appName.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pyst.productivist",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Productivist uses calendar functionality to schedule tasks, routines, appts, etc."
      }
    },
    "android": {
      "package": "com.pyst.productivist",
      "adaptiveIcon": {
        "foregroundImage": "./assets/launcherIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 3,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
